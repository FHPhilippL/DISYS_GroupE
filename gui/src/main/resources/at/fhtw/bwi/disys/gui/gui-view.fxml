<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="299.0" prefWidth="302.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="at.fhtw.bwi.disys.gui.GuiController">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="157.0" minWidth="10.0" prefWidth="127.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="198.0" minWidth="10.0" prefWidth="175.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
   <children>
      <Label prefHeight="17.0" prefWidth="93.0" text="Community Pool" />
      <Label fx:id="CommunityPoolUsageText" text="used" GridPane.columnIndex="1" />
      <Label text="Grid Portion" GridPane.rowIndex="1" />
       <Label fx:id="GridPortionPercentageText" text="\%" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="RefreshButton" mnemonicParsing="false" onAction="#onRefreshButtonClick" text="refresh" GridPane.rowIndex="2" />

      <Label text="Start" GridPane.rowIndex="3" />
       <DatePicker fx:id="StartTimeDatePicker" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Label text="End" GridPane.rowIndex="4" />
       <DatePicker fx:id="EndTimeDatePicker" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Button fx:id="ShowDataButton" mnemonicParsing="false" onAction="#onShowDataButtonClick" text="show data" GridPane.rowIndex="5" />

      <Label text="Community produced" GridPane.rowIndex="6" />
      <Label fx:id="CommunityProducedText" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6" />
      <Label text="Community used" GridPane.rowIndex="7" />
      <Label fx:id="CommunityUsedText" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="7" />
      <Label text="Grid used" GridPane.rowIndex="8" />
      <Label fx:id="GridUsedText" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="8" />
   </children>
</GridPane>
